<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<div class="phone_size">
  	<div class="contain">
	    <div class="contain_box">
	      	<div class="header_box">비밀번호 찾기</div>
	    </div>
	    <div class="msg_div checked_box"></div>
	    <div class="contain_box checked_box">
	      	<span class="icon-box">
	        	<label for="email">
	          	<i class="fa fa-envelope-o" aria-hidden="true"></i>
	        	</label>
	      	</span>
	      	<input class="login_input" id="email" placeholder="이메일" />
	    </div>
	    <div class="msg_div checked_box"></div>
	    <div class="contain_box checked_box">
	      	<span class="icon-box">
	        	<label for="tel">
	          	<i class="fa fa-mobile fa-lg" aria-hidden="true"></i>
	        	</label>
	      	</span>
	      	<input class="login_input" id="tel" placeholder="전화번호" />
	      	<div class="check_btn" id="check_num">인증받기</div>
	    </div>
	    <div class="msg_div checked_box"></div>
	    <div class="contain_box checked_box">
	      	<input class="login_input" type="text" id="checkNum" placeholder="인증번호" />
	    </div>
	    <div class="msg_box"></div>
	    <div class="msg_box">가입하신 ID로 비밀번호를 전송했습니다.</div>
	    <div class="msg_box"></div>
	    <div class="contain_box">
	     	<button class="login_button" id="next">다음</button>
	      	<button class="login_button" id="findPw_login">로그인하기</button>
    	</div>
  	</div>
</div>